void vPtr(void* ptr, char cmd) {
    if (cmd == 'i') {
        int* intPtr = static_cast<int*>(ptr);
        for (int i = 0; i < *intPtr; i++) {
            std::cout <<"ing"<< std::endl;
        }
    } else if (cmd == 'c') {
        char* charPtr = static_cast<char*>(ptr);
        std::cout <<"ceng "<< *charPtr << std::endl;
    } else {
        std::cout <<"Invalid command!"<< std::endl;
    }
}
