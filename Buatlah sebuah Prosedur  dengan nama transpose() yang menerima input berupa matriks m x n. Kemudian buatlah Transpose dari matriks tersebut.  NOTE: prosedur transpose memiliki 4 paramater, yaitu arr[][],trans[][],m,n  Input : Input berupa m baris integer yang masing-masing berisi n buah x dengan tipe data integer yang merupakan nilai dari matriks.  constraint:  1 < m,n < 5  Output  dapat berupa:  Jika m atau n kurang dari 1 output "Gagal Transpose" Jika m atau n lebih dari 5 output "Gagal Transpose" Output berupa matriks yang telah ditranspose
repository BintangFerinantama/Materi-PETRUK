#include <iostream>
using namespace std;

void transpose(int arr[][5], int trans[][5], int m, int n){
    
    if(m<1 || n<1){
        cout << "Gagal Transpose";
        return;
    }
    
    if(m>5 || n>5){
        cout << "Gagal Transpose";
        return;
    }
    
    for(int i=0; i<n; i++){
        for(int j=0; j<m; j++){
            trans[i][j] = arr[j][i];
        }
    }
    
    for(int i=0; i<n; i++){
        for(int j=0; j<m; j++){
            cout << trans[i][j] << " ";
        }
        cout << endl;
    }
}
